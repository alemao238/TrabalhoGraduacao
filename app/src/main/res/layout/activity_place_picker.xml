<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.developer.allef.projetomobile.PlacePickeeer"
    tools:layout="@layout/placespiick"
    >

    <Button
        android:layout_width="match_parent"
        android:text="Pesquisar Local"
        android:id="@+id/pla"
        android:layout_below="@+id/msg"
        android:layout_height="wrap_content"
        />

    <TextView
        android:layout_width="match_parent"
        android:id="@+id/msg"
        android:textSize="25dp"
        android:layout_height="wrap_content" />

    <Spinner
        android:id="@+id/bdd"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/pla"
        android:layout_height="wrap_content"></Spinner>


    <Spinner
        android:id="@+id/stipovaga"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/bdd"
        android:layout_marginTop="10dp"></Spinner>


    <Button
        android:layout_width="match_parent"
        android:text="Adicionar"
        android:id="@+id/placeBsalvar"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/stipovaga"
        android:layout_height="wrap_content" />

</RelativeLayout>
